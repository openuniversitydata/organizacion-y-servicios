 @prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix esadm: <http://vocab.linkeddata.es/datosabiertos/def/sector-publico/territorio#> .
@prefix geo: <http://www.owl-ontologies.com/geo#> .
@prefix geosparql: <http://www.owl-ontologies.com/geosparql#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix univCentro: <http://vocab.universidadesabiertas.es/ontology/organizacion-y-servicios#Centro> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dct:identifier a owl:DatatypeProperty ;
    rdfs:label "identifier" ;
    rdfs:domain univCentro:Universidad ;
    rdfs:range xsd:string .

esadm:municipio a owl:ObjectProperty ;
    rdfs:label "municipio" ;
    rdfs:domain univCentro:Campus,
        univCentro:Centro ;
    rdfs:range esadm:Municipio .

univCentro: a owl:Ontology ;
    dc:creator "VÃ­ctor Morcuende" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "Template Ontology" ;
    owl:versionInfo "0.0.3" .

univCentro:estaAsociadoA a owl:ObjectProperty ;
    rdfs:label "esta asociado a" ;
    rdfs:domain univCentro:CursoAcademico .

univCentro:fechaDesdeSituacionCentro a owl:DatatypeProperty ;
    rdfs:label "fecha desde situacion centro" ;
    rdfs:domain univCentro:Centro ;
    rdfs:range xsd:date .

univCentro:naturalezaCentro a owl:ObjectProperty ;
    rdfs:label "naturaleza centro" ;
    rdfs:domain univCentro:Centro .

univCentro:situacionCentro a owl:ObjectProperty ;
    rdfs:label "situacion centro" ;
    rdfs:domain univCentro:Centro .

univCentro:tipoCentro a owl:ObjectProperty ;
    rdfs:label "tipo centro" ;
    rdfs:domain univCentro:Centro .

geo:lat a owl:DatatypeProperty ;
    rdfs:label "lat" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:double .

geo:long a owl:DatatypeProperty ;
    rdfs:label "long" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:double .

geosparql:asWKT a owl:DatatypeProperty ;
    rdfs:label "as w k t" ;
    rdfs:domain geosparql:Geometry ;
    rdfs:range xsd:wKTLiteral .

geosparql:hasGeometry a owl:ObjectProperty ;
    rdfs:label "has geometry" ;
    rdfs:domain geosparql:Geometry ;
    rdfs:range geosparql:Feature .

org:hasSite a owl:ObjectProperty ;
    rdfs:label "has site" ;
    rdfs:domain org:Organization ;
    rdfs:range org:Site .

org:hasSubOrganization a owl:ObjectProperty ;
    rdfs:label "has sub organization" ;
    rdfs:domain univCentro:Universidad ;
    rdfs:range univCentro:Centro .

esadm:Municipio a owl:Class ;
    rdfs:label "Municipio" .

univCentro:CursoAcademico a owl:Class ;
    rdfs:label "Curso Academico" .

univCentro:estaUbicado a owl:ObjectProperty ;
    rdfs:label "esta ubicado" ;
    rdfs:domain univCentro:Centro ;
    rdfs:range univCentro:Campus .

org:Site a owl:Class ;
    rdfs:label "Site" ;
    rdfs:subClassOf geosparql:Feature .

univCentro:Campus a owl:Class ;
    rdfs:label "Campus" .

univCentro:Universidad a owl:Class ;
    rdfs:label "Universidad" ;
    rdfs:subClassOf org:Organization .

geosparql:Feature a owl:Class ;
    rdfs:label "Feature" ;
    rdfs:subClassOf geosparql:SpatialObject .

geosparql:Point a owl:Class ;
    rdfs:label "Point" ;
    rdfs:subClassOf geosparql:Geometry .

geosparql:SpatialObject a owl:Class ;
    rdfs:label "Spatial Object" .

geosparql:Geometry a owl:Class ;
    rdfs:label "Geometry" ;
    rdfs:subClassOf geosparql:SpatialObject .

org:Organization a owl:Class ;
    rdfs:label "Organization" .

univCentro:Centro a owl:Class ;
    rdfs:label "Centro" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univCentro:estaUbicado ],
        org:Organization .


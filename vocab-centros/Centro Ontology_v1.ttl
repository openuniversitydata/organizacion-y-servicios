 @prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix geo: <http://www.owl-ontologies.com/geo#> .
@prefix geosparql: <http://www.owl-ontologies.com/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix univCentro: <http://vocab.universidadesabiertas.es/ontology/organizacion-y-servicios#Centro> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

univCentro: a owl:Ontology ;
    dc:creator "VÃ­ctor Morcuende" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "Template Ontology" ;
    owl:versionInfo "0.0.1" .

univCentro:cod_campus a owl:DatatypeProperty ;
    rdfs:label "cod_campus" ;
    rdfs:domain univCentro:campus ;
    rdfs:range xsd:string .

univCentro:cod_centro a owl:DatatypeProperty ;
    rdfs:label "cod_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:string .

univCentro:cod_municipio a owl:DatatypeProperty ;
    rdfs:label "cod_municipio" ;
    rdfs:domain univCentro:municipio ;
    rdfs:range xsd:string .

univCentro:cod_naturaleza_centro a owl:DatatypeProperty ;
    rdfs:label "cod_naturaleza_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:integer .

univCentro:cod_situacion_centro a owl:DatatypeProperty ;
    rdfs:label "cod_situacion_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:integer .

univCentro:cod_tipo_centro a owl:DatatypeProperty ;
    rdfs:label "cod_tipo_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:string .

univCentro:cod_universidad a owl:DatatypeProperty ;
    rdfs:label "cod_universidad" ;
    rdfs:domain univCentro:universidad ;
    rdfs:range xsd:string .

univCentro:fecha_desde_situacion_centro a owl:DatatypeProperty ;
    rdfs:label "fecha_desde_situacion_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:date .

univCentro:naturaleza_centro a owl:DatatypeProperty ;
    rdfs:label "naturaleza_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:string .

univCentro:situacion_centro a owl:DatatypeProperty ;
    rdfs:label "situacion_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:string .

univCentro:tipo_centro a owl:DatatypeProperty ;
    rdfs:label "tipo_centro" ;
    rdfs:domain univCentro:centro ;
    rdfs:range xsd:string .

geo:lat a owl:DatatypeProperty ;
    rdfs:label "lat" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:decimal .

geo:long a owl:DatatypeProperty ;
    rdfs:label "long" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:decimal .

geosparql:asWKT a owl:DatatypeProperty ;
    rdfs:label "as w k t" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:wKTLiteral .

geosparql:hasGeometry a owl:ObjectProperty ;
    rdfs:label "has geometry" ;
    rdfs:domain geosparql:Feature ;
    rdfs:range geosparql:Geometry .

univCentro:curso_academico a owl:Class ;
    rdfs:label "curso_academico" .

univCentro:estaAsociadoA a owl:ObjectProperty ;
    rdfs:label "esta asociado a" ;
    rdfs:domain univCentro:campus,
        univCentro:curso_academico ;
    rdfs:range univCentro:municipio .

univCentro:estaUbicado a owl:ObjectProperty ;
    rdfs:label "esta ubicado" ;
    rdfs:domain univCentro:centro ;
    rdfs:range univCentro:municipio .

univCentro:posee a owl:ObjectProperty ;
    rdfs:label "posee" ;
    rdfs:domain univCentro:universidad ;
    rdfs:range univCentro:centro .

geosparql:Geometry a owl:Class ;
    rdfs:label "Geometry" ;
    rdfs:subClassOf geosparql:SpatialObject .

univCentro:campus a owl:Class ;
    rdfs:label "campus" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univCentro:estaAsociadoA ] .

univCentro:universidad a owl:Class ;
    rdfs:label "universidad" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univCentro:posee ] .

geosparql:Feature a owl:Class ;
    rdfs:label "Feature" ;
    rdfs:subClassOf geosparql:SpatialObject .

geosparql:SpatialObject a owl:Class ;
    rdfs:label "Spatial Object" .

univCentro:municipio a owl:Class ;
    rdfs:label "municipio" .

geosparql:Point a owl:Class ;
    rdfs:label "Point" ;
    rdfs:subClassOf geosparql:Point .

univCentro:centro a owl:Class ;
    rdfs:label "centro" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univCentro:estaUbicado ],
        geosparql:Feature .

